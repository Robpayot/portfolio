<div class="project__container">
	<div class="project__top">
		<div class="project__header">
			<h1 class="title--4">{{title}}</h1>
			<span class="project__date">{{date}}</span>
		</div>
		<p class="project__descr">{{descr}}</p>
		<div class="project__subHeader">
			<div class="project__context">
				<p>Agency</p>
				<p>{{context}}</p>
			</div>
			<div class="project__role">
				<p>Role</p>
				<p>Front-end</p>
			</div>
			<div class="project__technos">
				<p>In</p>
				<p>{{technos}}</p>
			</div>
			<div class="project__link">
				<span>Live</span>
				<a href="{{link}}" target="_blank" class="project__button">
					<svg class="is-live" viewBox="-744.2 -565.3 2098 2098" x="0px" y="0px">
						<polyline class="close-down" fill="none" stroke="#000000" stroke-width="83" stroke-miterlimit="10" points="612,484.7 290.1,483.7 72.1,698.4 "/>
						<polyline class="close-down-2" fill="none" stroke="#000000" stroke-width="83" stroke-miterlimit="10" points="612,484.7 290.1,483.7 72.1,698.4 "/>
						<polyline class="close-up" fill="none" stroke="#000000" stroke-width="83" stroke-miterlimit="10" points="72.1,257.2 300.7,483.7 606.7,483.7 "/>
						<circle fill="none" stroke="#000000" stroke-width="55" stroke-miterlimit="10" cx="304.8" cy="483.7" r="1021.5"/>
					</svg>
				</a>
			</div>
		</div>
	</div>
	<div class="project__gallery">
		{{#each imgs}}
			<div class="project__image"><img src="images/projects/{{this}}" alt="project image" /></div>
		{{/each}}
	</div>
	<div class="project__footer">
		<div class="project__awards">{{awards}}</div>
